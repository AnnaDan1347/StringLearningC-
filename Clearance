//#include "stdafx.h"
#include <iostream>
#include <stdio.h>
#include <conio.h>
using namespace std;
 
main() {
    int m[100][100];
    int i, j, k;
    int a, b, c;
    int n = 0;
    cout  << "Input n" << endl;
    cin >> n;
    for (i = 0; i < n; i++)
        for (j = 0; j < n*n; j++)
        {
            cin >> m[i][j];
        }
    //X
    for (i = 0; i < n; i++)
        for (a = 0; a < n; a++)  {
            k = 0;
            for (b = 0; b < n; b++)   {
                if (m[i][a*n+b] == 0) k+=1;
                if (k == n) 
cout << "Found clearance X. Coordinates x = " << 1 - n << "; y = " << a+1<< "; z =  "<< i+1<< endl;
            }
        }
    //z
    for (i = 0; i< n*n; i++)
    {
        k = 0;
        for (j = 0; j < n; j++)
        {
            if ( m[j][i] == 0 ) k+=1;
            if ( k == n ) 
cout << "Found clearance Z. Coordinates x = " << j<< "; y = " << i/n+1<< "; z =  "<< n<< endl;
        }
    }
    
    //y
    for (i = 0; i < n; i++)
        for (j = 0; j < n; j++) {
            
			if ( m[i][j] == 0 ) {
                k = 1;
                
				for (a = 0; a < n-1; a++) {
                    if (m[i][j+a] == 0) k+=1;
                }
                if (k == n) 
cout << "Found clearance Y. Coordinates x = " << j+1 << "; y = " << 2 << "; z =  "<< i+1 << endl;
        }
            }
        
    return 0;
}
